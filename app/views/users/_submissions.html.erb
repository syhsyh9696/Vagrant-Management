<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>ID</th>
      <th>Experiment ID</th>
      <th>Submission Status</th>
    </tr>
  </thead>
  <tbody>
    <tbody>
      <% @submissions.each do |submission| %>
        <tr>
          <td>#</td>
          <td><%= link_to submission.id, submission %></td>
          <td><%= link_to submission.experiment.title, submission.experiment %></td>
          <td><%= submission.status %></td>
        </tr>
      <% end %>
    </tbody>
</table>
<center>
<%= paginate @submissions, :theme => 'twitter-bootstrap-3' %>
</center>
